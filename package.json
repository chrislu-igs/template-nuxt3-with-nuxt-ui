    "@nuxtjs/i18n": "^9.2.1",
    "@vueuse/core": "12.7.0",
    "@vueuse/nuxt": "12.7.0",
    "eslint": "^9.21.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "nuxt": "^3.15.4",
